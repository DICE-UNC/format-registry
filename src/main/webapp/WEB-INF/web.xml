<!DOCTYPE web-app PUBLIC
          "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
          "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Format Registry</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>listclasses</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.ListClassesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newentity</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.NewEntityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filedownload</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.FileDownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listentities</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.ListEntitiesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayentity</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.DisplayEntityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>identify</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.FileIdentificationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>identifyresults</servlet-name>
        <servlet-class>edu.drexel.acin.sf.servlet.FileIdentificationPostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>listclasses</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayentity</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listentities</servlet-name>
        <url-pattern>/items</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filedownload</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newentity</servlet-name>
        <url-pattern>/newentity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>identify</servlet-name>
        <url-pattern>/identify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>identifyresults</servlet-name>
        <url-pattern>/identify-results</url-pattern>
    </servlet-mapping>
</web-app>

